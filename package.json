{
  "name": "telicent-instance",
  "private": true,
  "version": "1.0.4-dev.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "bump:prerelease": "yarn version --prerelease && git push && git push --tags",
    "bump:pre": "yarn bump:prerelease",
    "build": "tsc && vite build",
    "build:tailwind": "tailwindcss -i ./src/main.tailwind-source.css -o ./public/main.css",
    "test": "vitest",
    "test:ci": "vitest --silent --coverage",
    "test:main": "./scripts/test/changedSince.sh",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install",
    "predev": "yarn build:tailwind && ./scripts/try:build:env-config",
    "prebuild": "yarn build:tailwind && ./scripts/try:build:env-config",
    "postinstall": "[ \"$LOCAL_MACHINE\" = \"false\" ] && echo 'Skipping tefe hook-postinstall' || tefe hook-postinstall"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@rdfjs/types": "^1.1.0",
    "@telicent-oss/ds": "^0.0.4-rc.12",
    "@telicent-oss/ontologyservice": "^0.0.6",
    "@telicent-oss/rdfservice": "^0.0.6",
    "@telicent-oss/telicent-frontend-cli": "1.1.3",
    "@types/n3": "^1.16.4",
    "classnames": "^2.5.1",
    "dagre": "^0.8.5",
    "fontawesome": "^5.6.3",
    "inversify": "^6.0.2",
    "mobx": "^6.12.1",
    "mobx-react": "^9.1.0",
    "monaco-editor": "^0.47.0",
    "n3": "^1.17.3",
    "rdf-parse": "^2.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.11.1",
    "readable-stream": "^4.5.2",
    "reflect-metadata": "^0.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rdfjs/data-model": "^2.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@types/dagre": "^0.7.52",
    "@types/rdfjs__data-model": "^2.0.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": ">=6",
    "jsdom": "^24.0.0",
    "lint-staged": ">=10",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^1.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "engine": {
    "node": "v18.19.0"
  }
}
